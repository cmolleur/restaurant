<div class="rows">
  <div class="nine columns">
    <h1>Parties</h1>
  </div>
  <div class="three columns">
    <%= button_to "ADD NEW", new_party_path, :method => :get %>
  </div>

</div>
  <% @parties.each do |party| %>
  <div class="rows">
    <div class="nine columns">
      <h3>Group #: <%= party.id %></h3>
    </div>
    <div class="two columns">
      <%= button_to "VIEW", party_path(party.id), :method => :get %>
    </div>
  </div>

  <% end %>
